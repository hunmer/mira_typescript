{
    "name": "n8n-nodes-mira-apis",
    "version": "1.0.0",
    "description": "n8n nodes for Mira App Server APIs",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "",
    "author": {
        "name": "Mira Team",
        "email": "liaoyanjie2000@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-repo/n8n-nodes-mira-apis.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes --write",
        "lint": "eslint nodes/**/*.ts package.json",
        "lintfix": "eslint nodes/**/*.ts package.json --fix",
        "mac_test": "rm -rf ./dist && npm install && npm run build && npm prune --production && npm link && N8N_LOG_OUTPUT=console N8N_LOG_LEVEL=debug N8N_PAYLOAD_SIZE_MAX=64 N8N_FORMDATA_FILE_SIZE_MAX=500 npx n8n",
        "win_test": "./win_test.ps1",
        "prepublishOnly": "npm run build && npm run lint -s"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/MiraApi.credentials.js",
            "dist/credentials/MiraLoginCredential.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MiraLoginNode/MiraLoginNode.node.js",
            "dist/nodes/MiraLogoutNode/MiraLogoutNode.node.js",
            "dist/nodes/MiraVerifyTokenNode/MiraVerifyTokenNode.node.js",
            "dist/nodes/MiraGetCodesNode/MiraGetCodesNode.node.js",
            "dist/nodes/MiraAdminList/MiraAdminList.node.js",
            "dist/nodes/MiraAdminCreate/MiraAdminCreate.node.js",
            "dist/nodes/MiraAdminUpdate/MiraAdminUpdate.node.js",
            "dist/nodes/MiraAdminDelete/MiraAdminDelete.node.js",
            "dist/nodes/MiraUser/MiraUser.node.js",
            "dist/nodes/MiraLibrary/MiraLibrary.node.js",
            "dist/nodes/MiraPlugin/MiraPlugin.node.js",
            "dist/nodes/MiraFile/MiraFile.node.js",
            "dist/nodes/MiraDatabase/MiraDatabase.node.js",
            "dist/nodes/MiraDevice/MiraDevice.node.js",
            "dist/nodes/MiraSystem/MiraSystem.node.js"
        ]
    },
    "dependencies": {
        "form-data": "^4.0.0"
    },
    "devDependencies": {
        "@types/form-data": "^2.2.1",
        "@types/lodash": "^4.14.0",
        "@types/node": "^18.19.123",
        "@types/semver": "^7.5.0",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "form-data": "^4.0.0",
        "gulp": "^4.0.2",
        "n8n-core": "^1.14.1",
        "n8n-workflow": "^1.82.0",
        "prettier": "^2.7.1",
        "typescript": "^5.1.3"
    },
    "peerDependencies": {
        "n8n-core": "^1.14.1",
        "n8n-workflow": "^1.82.0"
    }
}