{
    "name": "n8n-nodes-mira-apis",
    "version": "1.0.0",
    "description": "n8n nodes for Mira App Server APIs",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "",
    "author": {
        "name": "Mira Team",
        "email": ""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-repo/n8n-nodes-mira-apis.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes --write",
        "lint": "eslint nodes/**/*.ts package.json",
        "lintfix": "eslint nodes/**/*.ts package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -s"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/MiraApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MiraAuth/MiraAuth.node.js",
            "dist/nodes/MiraUser/MiraUser.node.js",
            "dist/nodes/MiraAdmin/MiraAdmin.node.js",
            "dist/nodes/MiraLibrary/MiraLibrary.node.js",
            "dist/nodes/MiraPlugin/MiraPlugin.node.js",
            "dist/nodes/MiraFile/MiraFile.node.js",
            "dist/nodes/MiraDatabase/MiraDatabase.node.js",
            "dist/nodes/MiraDevice/MiraDevice.node.js",
            "dist/nodes/MiraSystem/MiraSystem.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^18.16.16",
        "@types/uuid": "^9.0.0",
        "@types/lodash": "^4.14.0",
        "@types/semver": "^7.5.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "gulp": "^4.0.2",
        "n8n-core": "^1.14.1",
        "n8n-workflow": "^1.82.0",
        "prettier": "^2.7.1",
        "typescript": "^5.1.3"
    },
    "peerDependencies": {
        "n8n-core": "^1.14.1",
        "n8n-workflow": "^1.82.0"
    }
}