{
    "name": "mira-server-sdk-examples",
    "version": "1.0.0",
    "description": "Examples and test cases for Mira Server SDK",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "examples",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "example": "node run-examples.js",
        "example:auth": "node run-examples.js auth",
        "example:upload": "node run-examples.js upload",
        "example:basic": "node run-examples.js basic",
        "example:advanced": "node run-examples.js advanced",
        "example:all": "node run-examples.js all",
        "test:auth": "jest tests/auth.test.ts",
        "test:upload": "jest tests/upload.test.ts",
        "lint": "eslint . --ext .ts,.js --fix",
        "dev": "npm run build:watch",
        "start": "node run-examples.js",
        "install:sdk": "cd ../mira-server-sdk && npm install && npm run build",
        "setup": "npm run install:sdk && npm install && npm run build",
        "demo": "npm run example:all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hunmer/mira_typescript.git",
        "directory": "packages/mira-server-sdk-examples"
    },
    "keywords": [
        "mira",
        "sdk",
        "examples",
        "typescript",
        "testing",
        "auth",
        "upload"
    ],
    "author": "Mira Team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hunmer/mira_typescript/issues"
    },
    "homepage": "https://github.com/hunmer/mira_typescript/tree/main/packages/mira-server-sdk-examples#readme",
    "dependencies": {
        "@types/form-data": "^2.2.1",
        "chalk": "^4.1.2",
        "dotenv": "^16.4.5",
        "form-data": "^4.0.4",
        "mira-server-sdk": "file:../mira-server-sdk"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.14.10",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "rimraf": "^5.0.7",
        "ts-jest": "^29.2.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}