{
  "name": "mira-app-server",
  "version": "1.0.6",
  "description": "Mira Server - standalone server application using mira-core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mira-server": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:ts": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mira",
    "server"
  ],
  "author": "hunmer",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "mira-app-core": "^1.0.4"
  }
}
