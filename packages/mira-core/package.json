    "@types/ws": "^8.18.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "express": "^5.1.0",
    "mira-storage-sqlite": "^1.0.10",
    "multer": "^2.0.2",
    "queue": "^7.0.0",
    "ws": "^8.18.3"
  }
