{
  "name": "n8n-nodes-mira-ws-trigger",
  "version": "0.1.2",
  "description": "Mira WebSocket Trigger node for n8n - Listen to real-time events from Mira server",
  "license": "MIT",
  "homepage": "https://github.com/user/mira_typescript",
  "author": {
    "name": "Mira Team",
    "email": "team@mira.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/mira_typescript.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "powershell -Command \"Copy-Item -Path 'nodes/MiraWebSocketTrigger/*.json' -Destination 'dist/nodes/MiraWebSocketTrigger/' -Force; Copy-Item -Path 'nodes/MiraWebSocketTrigger/*.svg' -Destination 'dist/nodes/MiraWebSocketTrigger/' -Force\"",
    "dev": "tsc -w",
    "prepack": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "websocket",
    "trigger",
    "mira",
    "real-time"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/MiraWebSocketTrigger/MiraWebSocketTrigger.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0"
  },
  "dependencies": {
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.4.0"
  }
}
